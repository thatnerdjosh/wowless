{
  "build": {
    "context": "..",
    "dockerfile": "Dockerfile"
  },
  "initializeCommand": "cp .env.dist .env",
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig",
        "ms-vscode.cpptools",
        "qwtel.sqlite-viewer"
      ]
    }
  },
  "name": "wowless/wowless",
  "postCreateCommand": "pre-commit install --install-hooks",
  "remoteUser": "vscode",
  "containerUser": "vscode",
  "workspaceMount": "",
  "runArgs": ["--volume=${localWorkspaceFolder}:/workspaces/${localWorkspaceFolderBasename}:Z", "--env-file", ".env", "--userns=keep-id"]
}
